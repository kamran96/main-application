{
  "version": 2,
  "projects": {
    "auth-middleware": {
      "root": "libs/auth-middleware",
      "sourceRoot": "libs/auth-middleware/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/auth-middleware/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/auth-middleware"],
          "options": {
            "jestConfig": "libs/auth-middleware/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "contacts-service": {
      "root": "apps/contacts-service",
      "sourceRoot": "apps/contacts-service/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/contacts-service",
            "main": "apps/contacts-service/src/main.ts",
            "tsConfig": "apps/contacts-service/tsconfig.app.json",
            "assets": ["apps/contacts-service/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/contacts-service/src/environments/environment.ts",
                  "with": "apps/contacts-service/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "contacts-service:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/contacts-service/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/contacts-service"],
          "options": {
            "jestConfig": "apps/contacts-service/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
