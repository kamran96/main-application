    "bank-reconcilation": {
      "root": "apps/bank-reconcilation",
      "sourceRoot": "apps/bank-reconcilation/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/bank-reconcilation",
            "index": "apps/bank-reconcilation/src/index.html",
            "baseHref": "/",
            "main": "apps/bank-reconcilation/src/main.tsx",
            "polyfills": "apps/bank-reconcilation/src/polyfills.ts",
            "tsConfig": "apps/bank-reconcilation/tsconfig.app.json",
            "assets": [
              "apps/bank-reconcilation/src/favicon.ico",
              "apps/bank-reconcilation/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "apps/bank-reconcilation/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bank-reconcilation/src/environments/environment.ts",
                  "with": "apps/bank-reconcilation/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "bank-reconcilation:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "bank-reconcilation:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/bank-reconcilation/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bank-reconcilation"],
          "options": {
            "jestConfig": "apps/bank-reconcilation/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
