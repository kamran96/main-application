{
  "version": 2,
  "projects": {
    "editable-table": {
      "root": "libs/editable-table",
      "sourceRoot": "libs/editable-table/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/editable-table/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/editable-table"],
          "options": {
            "jestConfig": "libs/editable-table/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/editable-table/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/editable-table",
            "config": {
              "configFolder": "libs/editable-table/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "editable-table-e2e": {
      "root": "apps/editable-table-e2e",
      "sourceRoot": "apps/editable-table-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/editable-table-e2e/cypress.json",
            "tsConfig": "apps/editable-table-e2e/tsconfig.e2e.json",
            "devServerTarget": "editable-table:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "editable-table:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/editable-table-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "main-api": {
      "root": "apps/main-api",
      "sourceRoot": "apps/main-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/main-api",
            "main": "apps/main-api/src/main.ts",
            "tsConfig": "apps/main-api/tsconfig.app.json",
            "assets": ["apps/main-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/main-api/src/environments/environment.ts",
                  "with": "apps/main-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "main-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/main-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/main-api"],
          "options": {
            "jestConfig": "apps/main-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "main-ui": {
      "root": "apps/main-ui",
      "sourceRoot": "apps/main-ui/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/main-ui",
            "index": "apps/main-ui/src/index.html",
            "main": "apps/main-ui/src/main.tsx",
            "polyfills": "apps/main-ui/src/polyfills.ts",
            "tsConfig": "apps/main-ui/tsconfig.app.json",
            "assets": [
              "apps/main-ui/src/favicon.ico",
              "apps/main-ui/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "apps/main-ui/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/main-ui/src/environments/environment.ts",
                  "with": "apps/main-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "main-ui:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "main-ui:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/main-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/main-ui"],
          "options": {
            "jestConfig": "apps/main-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "main-ui-e2e": {
      "root": "apps/main-ui-e2e",
      "sourceRoot": "apps/main-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/main-ui-e2e/cypress.json",
            "tsConfig": "apps/main-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "main-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "main-ui:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/main-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "pixels-to-rem": {
      "root": "libs/pixels-to-rem",
      "sourceRoot": "libs/pixels-to-rem/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/pixels-to-rem/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/pixels-to-rem"],
          "options": {
            "jestConfig": "libs/pixels-to-rem/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "review-invyce-theme": {
      "root": "libs/review/invyce-theme",
      "sourceRoot": "libs/review/invyce-theme/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/review/invyce-theme/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/review/invyce-theme"],
          "options": {
            "jestConfig": "libs/review/invyce-theme/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-global-state-manager": {
      "root": "libs/shared/global-state-manager",
      "sourceRoot": "libs/shared/global-state-manager/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/global-state-manager/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/global-state-manager"],
          "options": {
            "jestConfig": "libs/shared/global-state-manager/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-invyce-theme": {
      "root": "libs/shared/invyce-theme",
      "sourceRoot": "libs/shared/invyce-theme/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/invyce-theme/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/invyce-theme"],
          "options": {
            "jestConfig": "libs/shared/invyce-theme/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-types": {
      "root": "libs/shared/types",
      "sourceRoot": "libs/shared/types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/types/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/types"],
          "options": {
            "jestConfig": "libs/shared/types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-utils": {
      "root": "libs/shared/utils",
      "sourceRoot": "libs/shared/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/utils/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/utils"],
          "options": {
            "jestConfig": "libs/shared/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sidebar-ui": {
      "root": "libs/sidebar-ui",
      "sourceRoot": "libs/sidebar-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/sidebar-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/sidebar-ui"],
          "options": {
            "jestConfig": "libs/sidebar-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "main-api",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  }
}
